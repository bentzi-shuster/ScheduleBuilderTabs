{
  "name": "NJIT Schedule Builder Tabs",
  "manifest_version": 3,
  "version": "0.0.5",
  "permissions": ["tabs","webRequest","scripting"],
  "description": "This extension adds a tab bar to NJIT's schedule builder. Tabs can save a schedule that is currently being built.",
  "content_scripts": [    {
      "matches": ["https://uisapppr3.njit.edu/scbldr/"],
      "css": ["styles.css"],
      "js": ["scriptInject.js","messagePass.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
    
  },
  "web_accessible_resources": [{
    "resources": ["main.js","tabs.js","utilFuncs.js"],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "16": "images/16.png"
  }
}